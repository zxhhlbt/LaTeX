
@article{
	谢晶-116,
	Author = {谢晶},
	Title = {基于机器视觉的道路识别技术专利布局与发展趋势},
	Journal = {现代信息科技},
	Volume = {3},
	Number = {06},
	Pages = {174-177},
	Note = {页数: 4},
	Abstract = {道路识别技术是高级辅助驾驶系统(ADAS)中的关键部分,它能够为司机或者车辆的驾驶决策部分提供必要的道路环境信息。由于道路形态和路面状况的多样性,道路检测的结果易于受到光照、车辆,以及各种复杂的交通状况的影响,道路检测是一个非常复杂的问题,值得我们进行深入的研究。本文将从专利角度系统梳理有关道路识别技术的专利申请,对该领域的专利申请趋势、重要申请人及其研究重点进行总结,以期对该领域的专利审查带来有益的帮助。},
	Keywords = {机器视觉;道路识别;专利布局},
	Year = {2019} }




@article{
	谌华郭伟-117,
	Author = {谌华 and 郭伟 and 闫敬文 and 卓文浩 and 吴良斌},
	Title = {基于深度学习的SAR图像道路识别新方法},
	Journal = {吉林大学学报(工学版)},
	Pages = {1-10},
	Note = {页数: 10},
	Abstract = {深度学习是提高遥感图像目标识别精度的有效技术方法，针对传统合成孔径雷达(SAR)图像道路识别步骤繁杂问题，本文提出了一种新的基于深度学习的SAR图像道路识别方法。首先，在原有全卷积神经网络 (FCN)的基础上通过改进激活函数构造一种新的卷积神经网络M-FCN，有效缓解道路信息丢失问题。然后，将该卷积神经网络和自主构建的道路标签集应用于模拟SAR和真实SAR图像道路识别实验中， 提高鲁棒性。实验结果表明：与支持向量机(SVM)、传统全卷积神经网络和其它算法比较，该算法可以用来识别道路特征，并具有较高的精度和可靠性。},
	Keywords = {信息处理技术;合成孔径雷达图像;道路识别;深度学习;全卷积神经网络;最大映射特征},
	Year = {2019} }




@article{
	薛博元方艳-119,
	Author = {薛博元 and 方艳},
	Title = {无人驾驶汽车道路检测与识别方法研究},
	Journal = {南方农机},
	Number = {1},
	Pages = {207},
	DOI = {10.3969/j.issn.1672-3872.2019.01.167},
	Abstract = {基于对当前无人驾驶汽车道路检测与识别方法的研究, 本文指出了这些技术的实现过程, 并在此基础上对该项技术今后的发展前景和发展路径进行展望, 从而让该项技术在应用过程中能够更好的运行.},
	Keywords = {无人驾驶技术; 道路检测; 道路识别},
	Year = {2019} }




@phdthesis{
	冯黎-118,
	Author = {冯黎},
	Title = {无人驾驶汽车自主导航策略与控制算法研究},
	School = {电子科技大学},
	Note = {期: 12},
	Abstract = {无人驾驶技术拥有广阔的应用前景,具有重构未来整个交通体系的巨大潜力。随着技术的不断进步,人们对无人驾驶的未来充满了期待。“无人驾驶”即是要实现没有人参与控制的自主导航,无人驾驶的自主导航系统因其面临的环境的复杂性成为研究的热点。这其中,面临两大主要问题,一是信息源的提供的数据不够精确,不够稳定且不够全面,不能为决策提供有效的数据支持。二是难以构建能适应各种环境的决策的模型。为此,本文立足于无人驾驶复杂的环境,根据复杂适应系统的相关理论,构建了无人驾驶自主导航系统,以实现无人驾驶的自主导航。首先,针对无人驾驶汽车在复杂多变的环境自主导航问题,基于Agent对复杂系统建模的方式,构建了基于Agent的自主导航系统。自主导航系统中,主要包括场景转换模块、信息集模块和策略集模块。信息集模块主要通过信息融合的方式解决决策数据的支持问题。策略集模块为复杂环境下的自主导航提供策略支持。然后,对融合得到信息集模块中的自定位信息进行研究。原始的传感器信息有不确定性和感知有限性等缺点,通过冗余的传感器可以增强信息精度和鲁棒性,但传感器信息的相关性通常未知。对此本文提出了基于GPS、SINS和双目视觉里程计的分散式融合架构,有效的提高了自定位信息的精度和鲁棒性。接下来,对策略集模块中的基于规则的自主导航策略进行研究。设计了在结构化道路基于规则的自主导航策略。在规则约束下,基于时间窗生成轨迹,把复杂决策问题转化为轨迹的目标状态确定和轨迹评价问题,根据跟车模型自适应速度和安全距离。通过量化行车实时风险和舒适度对轨迹进行优选。最终实现了在复杂结构化道路进行换道、减速和加速等决策,实现了复杂结构化道路的自主导航。最后,对策略集模块中的基于学习的自主导航策略进行研究。把复杂环境下的决策模型建模为神经网络,根据复杂适应系统的演化规律,基于Unity3D构建非结构化道路训练场景,使用遗传算法来进化决策神经网络。通过逐步增加场景的复杂性,实现了无人驾驶汽车学会转弯,学会避障,学会驾驶。最后通过在场景中加入多个具有学习能力的无人驾驶汽车共同演化,来模拟高度复杂的非结构化道路。最终实现了复杂非结构化道路的自主导航。},
	Keywords = {自主导航系统;双目视觉里程计;复杂适应系统;遗传算法},
	Year = {2019} }




@phdthesis{
	贾会群-120,
	Author = {贾会群},
	Title = {无人驾驶车辆自主导航关键技术研究},
	School = {中国科学院大学(中国科学院长春光学精密机械与物理研究所)},
	Note = {期: 08},
	Abstract = {无人驾驶车辆是计算机科学、模式识别以及智能控制技术发展的产物,在城市交通、安保以及军事巡逻等方面具有广阔的应用前景。为了缓解城市交通压力,减少城市交通事故,用于城市交通和安保领域的无人驾驶车辆是目前的研究热点。城市交通领域和安保领域中的无人驾驶车辆,应具有独立自主执行任务的能力,因此,无人驾驶车辆在城市道路环境下的独立自主导航是自主导航技术的重要发展方向。无人驾驶车辆的自主导航的本质是指在没有人为干预的情况下车辆自主安全到达指定目的地,其主要涉及的研究方向包括环境感知、路径规划、决策控制和定位导航等,其中环境感知和路径规划是最基本、最关键的问题,因此本文重点对这两个方向开展研究。环境感知是指无人驾驶车辆利用多种探测设备(如相机、激光、雷达等),感知车辆外部的环境。环境感知中以相机等视觉传感器探测实现自主导航的无人驾驶车辆称为基于视觉导航的无人驾驶车辆。相比基于激光雷达等探测设备,视觉感知设备不仅价格低廉,且还具有为无人驾驶车辆提供丰富的外部环境信息的能力,有利于无人驾驶车辆感知外部环境,因此基于视觉导航的无人驾驶车辆是目前的研究热点。应用于城市环境下的无人驾驶车辆,基于视觉的环境感知的主要任务是检测和识别出周围的城市道路环境,包括车道线检测识别,路面标志检测识别和道路障碍检测。由于城市道路复杂的交通环境和不可控的自然环境的影响,导致检测识别算法在检测识别目标时,出现目标检测识别率不高,算法实时性差等问题。路径规划是指无人驾驶车辆根据环境感知得到的信息,规划出一条从任务起点运行到终点的合理路线。目前路径规划算法存在搜索精度低、特定情况下的搜索停滞等问题,导致路径规划精度低和实时性差。针对上述问题,本文以视觉导航为主线,在现有研究的基础上,对车道线检测算法、路面标志检测识别算法和道路障碍车辆检测算法以及路径规划算法开展研究,为无人驾驶车辆的自主导航提供有效的解决途径。论文的主要工作及创新之处包括:(1)车道线检测。详细分析了基于模型的车道线检测算法,包括直线模型、抛物线模型和三次曲线模型。针对目前算法的不足,提出了上凸曲线模型的车道线检测算法,算法首先使用上凸曲线模型对左右车道线进行检测,然后采用最小二乘法对检测结果进行拟合重构,实现高精度车道线检测,改善了目前算法应用范围窄,检测能力不足,鲁棒性差的问题。(2)箭头标志检测识别。主要研究了基于生物视觉感知模型和基于判别型表观模型的路面箭头标志检测识别算法,通过分析两...},
	Keywords = {无人驾驶车辆;环境感知;路径规划;检测识别;粒子群算法},
	Year = {2019} }


